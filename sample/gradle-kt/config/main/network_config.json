      }
    },
    {
      "name": "create_user",
      "url": "/user",
      "method": "POST",
      "headers": {
        "Content_Type": "application/json"
      },
      "body": {
        "username": "test",
        "password": "test"
      }
    },
    {
      "name": "update_user",
      "url": "/user/{id}",
      "method": "PUT",
